{
	"name": "clatelier",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.24.0",
	"engines": {
		"node": "24.11.1"
	},
	"scripts": {
		"dev": "pnpm --filter @clatelier/web dev",
		"build": "pnpm -r build",
		"typecheck": "pnpm -r typecheck",
		"lint": "biome check .",
		"format": "biome check --write .",
		"test": "pnpm -r test",
		"test:e2e": "pnpm --filter @clatelier/web test:e2e",
		"clean": "pnpm -r clean"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "lefthook"]
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@typescript/native-preview": "catalog:",
		"lefthook": "catalog:"
	}
}
