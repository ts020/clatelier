		"clean": "rm -rf dist"
	},
	"dependencies": {
		"hono": "catalog:",
		"@clatelier/shared": "workspace:*"
	},
	"devDependencies": {
		"typescript": "catalog:",
		"@types/node": "catalog:",
		"@typescript/native-preview": "catalog:",
		"tsup": "catalog:",
		"vitest": "catalog:"
	}
}
